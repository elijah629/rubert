{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"lib": ["ESNext", "DOM", "WebWorker"],
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
